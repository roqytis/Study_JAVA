package javaWorkshop02;

public class mytest06 {

	public static void main(String[] args) {
		for (int i = 1; i <= 6; i++) {
			for (int j = 1; j <= 6; j++) {
				for (int c = 1; c <=6; c++) {
					if((i*j*c)%3==0) {
						System.out.println(i+"*"+j+"*"+c+"="+i*j*c );
					}//if
				}//for
			}// for
		}//for
	}// main
}//class

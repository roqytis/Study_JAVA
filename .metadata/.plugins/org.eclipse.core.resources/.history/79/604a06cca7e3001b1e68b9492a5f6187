package javaWorkshop02;

import java.util.Scanner;

public class test13 {

	public static void main(String[] args) {
		Scanner scan =new Scanner(System.in);
		
		System.out.println("2자리의 정수를 입력하세요");
		int n;
		do { System.out.print("입력: ");
		n=scan.nextInt();
			
		} while (10>=n || n>100);

	}

}

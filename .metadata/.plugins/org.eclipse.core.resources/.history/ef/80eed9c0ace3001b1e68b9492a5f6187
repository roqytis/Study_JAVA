package javaWorkshop02;

import java.util.Scanner;

public class test14 {

	public static void main(String[] args) {
		Scanner scan =new Scanner(System.in);
		System.out.println("------곱셈표------");
		for (int i = 1; i <= 10; i++) {
			for (int j = 1; j <= 10; j++) {
				System.out.println(i*j);
			}
			
		}

	}

}
